{
  "name": "plosolver",
  "version": "1.0.0",
  "description": "Poker equity calculator and solver",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --mode development",
    "dev": "webpack serve --mode development",
    "start:ngrok": "webpack serve --mode development --host 0.0.0.0",
    "start:staging": "env-cmd -f env.staging-local webpack serve --mode development",
    "build:staging": "env-cmd -f env.staging-local webpack --mode production",
    "tunnel:start": "./scripts/connection-manager.sh start",
    "tunnel:stop": "./scripts/connection-manager.sh stop",
    "tunnel:status": "./scripts/connection-manager.sh status",
    "tunnel:test": "./scripts/connection-manager.sh test",
    "tunnel:setup": "./scripts/connection-manager.sh setup",
    "build": "webpack --mode production",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "typecheck:ci": "tsc -p tsconfig.ci.json --noEmit || true",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit",
    "test:selenium": "node __tests__/selenium/run-selenium-tests.js",
    "test:selenium:headless": "node __tests__/selenium/run-selenium-tests.js --headless",
    "test:selenium:chrome": "node __tests__/selenium/run-selenium-tests.js --browser=chrome",
    "test:selenium:firefox": "node __tests__/selenium/run-selenium-tests.js --browser=firefox",
    "test:selenium:plo-solver": "node __tests__/selenium/run-plo-solver-tests.js",
    "test:selenium:plo-solver:headless": "node __tests__/selenium/run-plo-solver-tests.js --headless",
    "test:selenium:user-workflow": "node __tests__/selenium/user-workflow-tests.js",
    "test:selenium:user-workflow:headless": "node __tests__/selenium/user-workflow-tests.js --headless",
    "test:selenium:all": "node __tests__/selenium/run-all-selenium-tests.js",
    "test:selenium:all:headless": "node __tests__/selenium/run-all-selenium-tests.js --headless",
    "test:selenium:comprehensive": "node __tests__/selenium/run-comprehensive-tests.js",
    "test:selenium:comprehensive:headless": "node __tests__/selenium/run-comprehensive-tests.js --headless",
    "test:selenium:plo-specific": "node __tests__/selenium/plo-specific-tests.js",
    "test:selenium:plo-specific:headless": "node __tests__/selenium/plo-specific-tests.js --headless",
    "test:selenium:master": "node __tests__/selenium/run-all-selenium-suites.js",
    "test:selenium:master:headless": "node __tests__/selenium/run-all-selenium-suites.js --headless",
    "test:selenium:master:parallel": "node __tests__/selenium/run-all-selenium-suites.js --parallel",
    "test:selenium:master:parallel:headless": "node __tests__/selenium/run-all-selenium-suites.js --parallel --headless",
    "test:selenium:staging": "SELENIUM_BASE_URL=http://staging.ploscope.com node __tests__/selenium/run-all-selenium-tests.js --headless",
    "test:selenium:production": "SELENIUM_BASE_URL=http://ploscope.com node __tests__/selenium/run-all-selenium-tests.js --headless",
    "test:cucumber": "npx cucumber-js __tests__/cucumber/features --profile development",
    "test:cucumber:headless": "CUCUMBER_HEADLESS=true npx cucumber-js __tests__/cucumber/features --profile headless",
    "test:cucumber:parallel": "npx cucumber-js __tests__/cucumber/features --profile development --parallel 2",
    "test:cucumber:parallel:headless": "CUCUMBER_HEADLESS=true npx cucumber-js __tests__/cucumber/features --profile headless --parallel 2",
    "test:cucumber:auth": "npx cucumber-js __tests__/cucumber/features --profile development --tags @authentication",
    "test:cucumber:solver": "npx cucumber-js __tests__/cucumber/features --profile development --tags @solver",
    "test:cucumber:plo": "npx cucumber-js __tests__/cucumber/features --profile development --tags @plo-specific",
    "test:cucumber:ui": "npx cucumber-js __tests__/cucumber/features --profile development --tags @ui",
    "test:cucumber:staging": "CUCUMBER_BASE_URL=http://staging.ploscope.com CUCUMBER_HEADLESS=true npx cucumber-js __tests__/cucumber/features --profile headless",
    "test:cucumber:production": "CUCUMBER_BASE_URL=http://ploscope.com CUCUMBER_HEADLESS=true npx cucumber-js __tests__/cucumber/features --profile headless",
    "test:cucumber:ci": "CUCUMBER_BASE_URL=http://traefik CUCUMBER_HEADLESS=true npx cucumber-js __tests__/cucumber/features --profile ci",
    "test:cucumber:local": "CUCUMBER_BASE_URL=http://localhost:3001 CUCUMBER_HEADLESS=false npx cucumber-js __tests__/cucumber/features --profile development",
    "test:cucumber:local-headless": "CUCUMBER_BASE_URL=http://localhost:3001 CUCUMBER_HEADLESS=true npx cucumber-js __tests__/cucumber/features --profile headless",
    "cucumber": "npx cucumber-js __tests__/cucumber/features --profile development",
    "cucumber:headless": "CUCUMBER_HEADLESS=true npx cucumber-js __tests__/cucumber/features --profile headless",
    "cucumber:ci": "CUCUMBER_BASE_URL=http://traefik CUCUMBER_HEADLESS=true npx cucumber-js __tests__/cucumber/features --profile ci",
    "cucumber:local": "CUCUMBER_BASE_URL=http://localhost:3001 npx cucumber-js __tests__/cucumber/features --profile development",
    "docs:generate": "python scripts/docs/generate_docs.py",
    "docs:api": "python src/backend/api_docs_generator.py",
    "docs:react": "node scripts/react_docs_generator.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:imports": "eslint . --ext .js,.jsx,.ts,.tsx --rule 'import/order: error'",
    "lint:imports:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix --rule 'import/order: error'",
    "sort-imports": "eslint src --ext .js,.jsx,.ts,.tsx --fix --rule 'import/order: error' --rule 'import/no-duplicates: error'",
    "sort-imports:all": "eslint . --ext .js,.jsx,.ts,.tsx --fix --rule 'import/order: error' --rule 'import/no-duplicates: error'",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "styles:dedupe": "node scripts/styles/deduplicateStyles.js",
    "sitemap": "node scripts/generate_sitemap.js",
    "postbuild": "npm run sitemap"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@cucumber/cucumber": "^12.2.0",
    "@google-cloud/local-auth": "^3.0.1",
    "@grafana/faro-web-sdk": "^1.6.0",
    "@grafana/faro-web-tracing": "^1.6.0",
    "@mdx-js/loader": "^3.0.1",
    "assert": "^2.1.0",
    "axios": "^1.7.9",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "engine.io-client": "^6.6.1",
    "events": "^3.3.0",
    "https-browserify": "^1.0.0",
    "invariant": "^2.2.4",
    "js-cookie": "^3.0.5",
    "process": "^0.11.10",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-helmet": "^6.1.0",
    "react-icons": "^5.5.0",
    "react-loading-indicators": "^1.0.1",
    "react-router-dom": "^7.6.2",
    "reflect-metadata": "^0.2.2",
    "rrweb": "^1.1.3",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@babel/register": "^7.27.1",
    "@cucumber/pretty-formatter": "^1.0.0",
    "@playwright/test": "^1.48.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.5",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^10.0.0",
    "chai": "^4.3.10",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "dependency-cruiser": "^17.0.2",
    "dotenv": "^17.0.1",
    "env-cmd": "^11.0.0",
    "eslint": "^8.57.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-security": "^2.1.0",
    "glob": "^10.0.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.5",
    "lint-staged": "^15.2.2",
    "madge": "^8.0.0",
    "mini-css-extract-plugin": "^2.9.4",
    "msw": "^2.8.0",
    "playwright": "^1.48.0",
    "postcss": "^8.4.35",
    "postcss-safe-parser": "^6.0.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.2.5",
    "puppeteer": "^24.10.2",
    "react-project-graph": "^1.0.0",
    "sass": "^1.89.2",
    "sass-graph": "^4.0.1",
    "sass-graph-viz": "^2.4.0",
    "sass-loader": "^16.0.5",
    "semantic-release": "^24.2.7",
    "style-loader": "^4.0.0",
    "typescript": "5.5.4",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "rootDir": ".",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "globalTeardown": "<rootDir>/teardownTests.js",
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "^(\\.\\./)*utils/testUtils$": "<rootDir>/__tests__/utils/testUtils.js",
      "^(\\.\\./)*utils/(.*)$": "<rootDir>/src/utils/$2",
      "^(\\.\\./)*components/(.*)$": "<rootDir>/src/components/$2",
      "^(\\.\\./)*pages/(.*)$": "<rootDir>/src/pages/$2",
      "^(\\.\\./)*hooks/(.*)$": "<rootDir>/src/hooks/$2",
      "^(\\.\\./)*contexts/(.*)$": "<rootDir>/src/contexts/$2",
      "^@/(.*)$": "<rootDir>/src/$1",
      "^@/components/(.*)$": "<rootDir>/src/components/$1",
      "^@/pages/(.*)$": "<rootDir>/src/pages/$1",
      "^@/utils/(.*)$": "<rootDir>/src/utils/$1",
      "^@/hooks/(.*)$": "<rootDir>/src/hooks/$1",
      "^@/contexts/(.*)$": "<rootDir>/src/contexts/$1",
      "^@/styles/(.*)$": "<rootDir>/src/styles/$1",
      "^@/types/(.*)$": "<rootDir>/src/types/$1"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testMatch": [
      "<rootDir>/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/**/*.{test,spec}.{js,jsx,ts,tsx}"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/utils/",
      "<rootDir>/__tests__/cucumber/"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!index.tsx",
      "!setupTests.js",
      "!__tests__/**",
      "!components/examples/**",
      "!coverage/lcov-report/**",
      "!.eslintrc.js",
      "!babel.config.js",
      "!webpack.config.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "transform": {
      "^.+\\.(ts|tsx|js|jsx)$": "babel-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "coverageProvider": "v8",
    "maxWorkers": 1,
    "workerIdleMemoryLimit": "512MB",
    "testTimeout": 30000,
    "forceExit": true,
    "detectOpenHandles": true
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings 0 --rule 'import/order: error' --rule 'import/no-duplicates: error'",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "overrides": {
    "glob": "^10.0.0",
    "inflight": "npm:@npmcli/fs@^3.1.0"
  }
}
